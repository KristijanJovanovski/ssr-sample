{
  "testURL": "http://localhost/",
  "collectCoverage": true,
  "moduleFileExtensions": ["ts", "tsx", "js"],
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  },
  "testMatch": ["**/src/**/*.test.(ts|tsx)?(x)"],
  "setupTestFrameworkScriptFile": "<rootDir>setUpTest.ts"
}
